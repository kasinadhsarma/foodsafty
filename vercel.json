{
    "version": 2,
    "builds": [
        {
            "src": "backend/app.py",
            "use": "@vercel/python",
            "config": {
                "runtime": "python3.10",
                "maxLambdaSize": "15mb"
            }
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "backend/app.py"
        }
    ],
    "env": {
        "PYTHONPATH": "backend"
    }
}
